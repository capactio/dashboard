    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "antd": "^4.17.2",
    "graphql": "^16.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.33.7",
    "react-router-dom": "^6.0.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "postinstall": "npm run setup-hooks",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint ./src",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-graphql": "set -o allexport;. .env; set +o allexport; graphql-codegen --config codegen.yml",
